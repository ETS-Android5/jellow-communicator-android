<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearlayout_icon1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginLeft="@dimen/nineIcon_parentHorizontal"
    android:layout_marginRight="@dimen/nineIcon_parentHorizontal"
    android:focusable="true">
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/frame_layout"
        app:layout_constraintLeft_toLeftOf="@+id/linearlayout_icon1"
        app:layout_constraintRight_toRightOf="@+id/linearlayout_icon1"
        app:layout_constraintTop_toTopOf="@+id/linearlayout_icon1">
        <ImageView
            android:id="@+id/icon1"
            android:layout_width="@dimen/nineIcon_width"
            android:layout_height="@dimen/nineIcon_width"
            android:layout_gravity="center_horizontal"
            android:background="@android:color/transparent"/>
        <View
            android:id="@+id/borderView"
            android:layout_width="@dimen/nineIcon_width"
            android:layout_height="@dimen/nineIcon_width"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/border_drawable_nocolor_nine_icon"/>
        <RelativeLayout
            android:layout_width="@dimen/nineIcon_width"
            android:visibility="gone"
            android:id= "@+id/edit_remove_container"
            android:layout_height="@dimen/nineIcon_width">
            <ImageView
                android:id="@+id/delete_icons"
                android:layout_width="@dimen/nineIcon_remove_btn"
                android:layout_height="@dimen/nineIcon_remove_btn"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:src="@drawable/ic_delete_icon_red_back" />
            <ImageView
                android:id="@+id/edit_icons"
                android:layout_width="@dimen/nineIcon_remove_btn"
                android:layout_height="@dimen/nineIcon_remove_btn"
                android:src="@drawable/ic_edit_icon_red_back"/>
        </RelativeLayout>
    </FrameLayout>
    <TextView
        android:id="@+id/te1"
        android:layout_width="@dimen/nineIcon_textWidth"
        android:layout_height="wrap_content"
        android:text="festivals &amp; hodidays"
        android:gravity="center"
        android:textSize="@dimen/nineIcon_textSize"
        android:textAlignment="center"
        android:lineSpacingExtra="1dp"
        android:lineSpacingMultiplier=".8"
        app:layout_constraintLeft_toLeftOf="@+id/linearlayout_icon1"
        app:layout_constraintRight_toRightOf="@+id/linearlayout_icon1"
        app:layout_constraintTop_toBottomOf="@+id/frame_layout"/>
</androidx.constraintlayout.widget.ConstraintLayout>